
<b>List of contacts of <%=@user[:email]%> obtained from gmail:</b>


<%= form_for [@party, @invitation] do |f| %>
  <% if @invitation.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@invitation.errors.count, "error") %> prohibited this invitation from being saved:</h2>

      <ul>
      <% @invitation.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.fields_for :user_attributes do |ff| %>
    <%= ff.collection_check_boxes(:email_list, @emails, :last, :last) %>
    <% end %>
  </div>


  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
